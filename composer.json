{
    "name": "echo-fution/echo-fusion",
    "description": "structure of echoFusion php framework",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "EchoFusion",
        "skeleton",
        "framework",
        "PHP"
    ],
    "support": {
        "issues": "https://github.com/echo-fusion/echo-fusion/issues",
        "source": "https://github.com/echo-fusion/echo-fusion/"
    },
    "authors": [
        {
            "name": "Amir Shadanfar",
            "email": "a.shadanfar.it@gmail.com"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
        "echo-fusion/framework": "0.x-dev"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../framework",
            "options": {
                "symlink": true
            }
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "dev-master",
        "echo-fusion/codestandard": "^1.0",
        "vimeo/psalm": "5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "App\\": "/app/",
            "Modules\\": "/modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "/tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
